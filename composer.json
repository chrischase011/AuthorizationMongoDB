{
    "name": "chrischase/authorizationmongodb",
    "description": "Native Laravel Authorization with MongoDB.",
    "keywords": [
        "authorization",
        "laravel",
        "permissions",
        "roles",
        "acl",
        "mongodb"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "illuminate/database": "^5.5|^6.20|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.7|^4.0|^5.0|^6.0|^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.0|^10.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "ChrisChase\\AuthorizationMongoDB\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ChrisChase\\AuthorizationMongoDB\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChrisChase\\AuthorizationMongoDB\\AuthorizationServiceProvider"
            ]
        }
    }
}
